    "@types/jest": "^23.3.5",
    "@types/mongoose": "^5.2.19",
    "@types/node": "^10.12.0",
    "gts": "^0.8.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "mongodb": "^3.1.8",
    "mongodb-memory-server": "^2.4.3",
