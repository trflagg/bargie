{
  "name": "bargie",
  "version": "1.0.0",
  "description": "Backend interactive engine",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch-coverage": "jest --watch --coverage",
